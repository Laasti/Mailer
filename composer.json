    "keywords": ["mail", "smtp", "sendmail", "email", "mailer"],
    "description": "A very lightweight PHP email sender",
    "license": "MIT",
    "homepage": "http://github.com/laasti/Mailer",
    "authors": [
      {
        "name": "Sonia Marquette",
